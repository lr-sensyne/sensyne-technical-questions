{
  "name": "fetechtest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "^27.0.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/material-ui": "^0.21.12",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.15",
    "axios": "^0.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4",
    "uuidv4": "^6.2.12"
  },
  "scripts": {
    "start": "concurrently \"yarn start:server\" \"yarn start:app\"",
    "start:app": "react-scripts start",
    "start:server": "json-server --watch db.json --port 3001 --quiet",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-fix": "node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "format": "node_modules/.bin/prettier --check ./src/**/*.{ts,tsx} ./src/**/*.*.{ts,tsx}",
    "format-fix": "node_modules/.bin/prettier --write ./src/**/*.{ts,tsx} ./src/**/*.*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "json-server": "^0.15.1"
  }
}
